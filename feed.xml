---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title }}</title>
    <description>{{ site.description }}</description>
    <link>{{ site.url }}/</link>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    <language>hy</language>
    <atom:link href="{{ site.url }}/feed.xml" rel="self" type="application/rss+xml"/>
    <copyright> Â© {{ 'now' | date: '%Y' }} {{ site.author }} </copyright>
    {% for post in site.posts limit: 30 %}
      {% assign post_absolute_url = site.url | append: post.url | url_decode %}
      {% assign imgTag = "<img src='{{site.url}}{{post.photo}}' />" %}
      <item>
        <title>{{ post.title | xml_escape}}</title>
        <link>{{ post_absolute_url }}</link>
        <description>
        {{ imgTag | xml_escape }}
       </description>
        <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
        <guid isPermaLink="true">{{ post_absolute_url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
